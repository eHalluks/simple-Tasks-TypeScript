{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["ES2022"],
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "downlevelIteration": true,
    "preserveConstEnums": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  },
}

